<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="140"
            android:toDegrees="40"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape
                android:shape="line"
                android:top="1dip">
                <stroke
                    android:width="3dip"
                    android:color="#dbf8cc09">
                </stroke>
            </shape>
        </rotate>
    </item>
</layer-list>